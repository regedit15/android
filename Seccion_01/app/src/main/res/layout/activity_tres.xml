<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ActivityTres">

    <EditText
        android:id="@+id/textUsuario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="162dp"
        android:ems="10"
        android:inputType="text" />

    <ImageButton
        android:id="@+id/btnCamara"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:background="#86105b"
        app:srcCompat="@android:drawable/ic_menu_camera"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="475dp" />

    <ImageButton
        android:id="@+id/btnCamara"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:onClick="btnCamaraClick"
        app:srcCompat="@android:drawable/ic_menu_camera"
        tools:layout_editor_absoluteX="312dp"
        tools:layout_editor_absoluteY="117dp" />

    <ImageButton
        android:id="@+id/btnUsuario2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/btnTelefono"
        android:layout_alignTop="@+id/textUsuario"
        android:onClick="btnBrowserClick"
        app:srcCompat="@android:drawable/ic_menu_myplaces"
        tools:layout_editor_absoluteX="312dp"
        tools:layout_editor_absoluteY="117dp" />

    <ImageButton
        android:id="@+id/btnTelefono"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/txtTelefono"
        android:layout_marginEnd="12dp"
        android:onClick="btnTelefonoClick"
        app:srcCompat="@android:drawable/ic_menu_call"
        tools:layout_editor_absoluteX="312dp"
        tools:layout_editor_absoluteY="41dp" />

    <EditText
        android:id="@+id/txtTelefono"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="88dp"
        android:ems="10"
        android:inputType="phone" />
</RelativeLayout>